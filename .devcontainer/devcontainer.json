{
	"image": "almalinux:9.2",
	"features": {},
	"customizations": {
		"vscode": {
			"extensions": [
				"EditorConfig.EditorConfig"
			]
		}
	},
	"mounts": [
		{
			"source": "devcontainer-pak-cache",
			"target": "/home/rstudio/.cache/R/pkgcache/pkg",
			"type": "volume"
		},
		{
			"source": "devcontainer-cargo-cache-${devcontainerId}",
			"target": "/usr/local/cargo",
			"type": "volume"
		},
		{
			"source": "/home/jshinonome/Workspace/kola",
			"target": "/workspaces/kola",
			"type": "bind"
		},
		{
			"source": "/home/jshinonome/.local/share/q401",
			"target": "/workspaces/q",
			"type": "bind"
		}
	],
	"updateContentCommand": {
		"setup-r-polars-requirements": "task setup-dev"
	}
}
